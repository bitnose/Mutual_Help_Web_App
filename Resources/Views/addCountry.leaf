#set("content") {
<h1>#(title)</h1>

<form method="post">

#if(csrfToken) {
<input type="hidden" name="csrfToken" value="#(csrfToken)">
}


<div class="form-group">
<label for="country">Country</label>
<input type="text" name="country" class="form-control"
id="country"/>
</div>


<button type="submit" class="btn btn-primary">
Add Country
</button>
</form>
}

#embed("base")
